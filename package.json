{
  "name": "waiterrobot-web",
  "version": "1.8.5",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --hmr",
    "start:lava": "ng serve --configuration lava --hmr",
    "build:prod": "rm -rf ./dist/WaiterRobot-Web/ && mkdir -p ./dist/WaiterRobot-Web && ng build --configuration production && cp ./LICENSE ./dist/WaiterRobot-Web/",
    "build:lava": "rm -rf ./dist/WaiterRobot-Web/ && mkdir -p ./dist/WaiterRobot-Web && ng build --configuration lava && cp ./LICENSE ./dist/WaiterRobot-Web/",
    "build:local": "rm -rf ./dist/WaiterRobot-Web/ && mkdir -p ./dist/WaiterRobot-Web && ng build --configuration local && cp ./LICENSE ./dist/WaiterRobot-Web/",
    "build:stats": "rm -rf ./dist/WaiterRobot-Web/ && mkdir -p ./dist/WaiterRobot-Web && ng build --stats-json",
    "analyze": "npm run build:stats && source-map-explorer dist/WaiterRobot-Web/**/*.js",
    "tsc:check": "tsc",
    "ci": "pnpm lint && pnpm format",
    "format:check": "prettier --check .",
    "format": "prettier --write .",
    "e2e": "ng e2e",
    "api:types": "swagger-typescript-api -p http://localhost:8080/api/swagger/api-docs -o ./src/app/_shared/ -n waiterrobot-backend.d.ts --no-client && prettier --write ./src/app/_shared/waiterrobot-backend.d.ts",
    "api:types:lava": "swagger-typescript-api -p https://lava.kellner.team/api/swagger/api-docs -o ./src/app/_shared/ -n waiterrobot-backend.d.ts --no-client && prettier --write ./src/app/_shared/waiterrobot-backend.d.ts"
  },
  "packageManager": "pnpm@9.5.0",
  "private": true,
  "dependencies": {
    "@angular-devkit/build-angular": "^18.0.0",
    "@angular/animations": "18.1.0",
    "@angular/cdk": "18.1.1",
    "@angular/common": "18.1.0",
    "@angular/compiler": "18.1.0",
    "@angular/core": "18.1.0",
    "@angular/forms": "18.1.0",
    "@angular/localize": "18.1.0",
    "@angular/platform-browser": "18.1.0",
    "@angular/platform-browser-dynamic": "18.1.0",
    "@angular/router": "18.1.0",
    "@jsverse/transloco": "7.4.2",
    "@micro-sentry/angular": "^7.0.2",
    "@ng-bootstrap/ng-bootstrap": "17.0.0",
    "@ng-select/ng-select": "^13.0.0",
    "@popperjs/core": "2.11.8",
    "@swimlane/ngx-charts": "20.5.0",
    "@types/tinycolor2": "^1.4.6",
    "bootstrap": "5.3.2",
    "d3-shape": "3.2.0",
    "dfts-helper": "1.1.1",
    "dfx-bootstrap-icons": "2.0.1",
    "dfx-bootstrap-table": "4.0.1",
    "dfx-helper": "8.0.1",
    "dfx-qrcode": "3.0.1",
    "html-to-image": "1.11.11",
    "jspdf": "2.5.1",
    "ng2-pdfjs-viewer": "^18.0.0",
    "ngx-print": "^1.5.1",
    "ngxtension": "4.0.0",
    "rxjs": "7.8.1",
    "tinycolor2": "^1.6.0",
    "tslib": "2.6.3",
    "zone.js": "0.14.7"
  },
  "devDependencies": {
    "@angular/cli": "18.1.1",
    "@angular/compiler-cli": "18.1.0",
    "@types/d3-scale": "4.0.8",
    "@types/d3-selection": "3.0.10",
    "@types/d3-shape": "^3.1.1",
    "@types/jasmine": "5.1.4",
    "@types/node": "^20.2.5",
    "jasmine-core": "5.1.2",
    "jasmine-spec-reporter": "7.0.0",
    "karma": "6.4.3",
    "karma-chrome-launcher": "3.2.0",
    "karma-coverage": "2.2.1",
    "karma-jasmine": "5.1.0",
    "karma-jasmine-html-reporter": "2.1.0",
    "license-report": "^6.4.0",
    "prettier": "3.3.3",
    "source-map-explorer": "2.5.3",
    "swagger-typescript-api": "^13.0.0",
    "typescript": "5.5.3"
  }
}
