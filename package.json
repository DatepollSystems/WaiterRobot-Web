{
  "name": "waiter-robot-web",
  "version": "0.1.0-beta",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build:prod": "rm -rf ./dist/WaiterRobot-Web/ && mkdir -p ./dist/WaiterRobot-Web && ng build --configuration production && cp ./LICENSE ./dist/WaiterRobot-Web/",
    "build:testing": "rm -rf ./dist/WaiterRobot-Web/ && mkdir -p ./dist/WaiterRobot-Web && ng build --configuration testing && cp ./LICENSE ./dist/WaiterRobot-Web/",
    "build:dev": "rm -rf ./dist/WaiterRobot-Web/ && mkdir -p ./dist/WaiterRobot-Web && ng build && cp ./LICENSE ./dist/WaiterRobot-Web/",
    "build:stats": "rm -rf ./dist/WaiterRobot-Web/ && mkdir -p ./dist/WaiterRobot-Web && ng build --stats-json",
    "analyze": "npm run build:dev && webpack-bundle-analyzer dist/WaiterRobot-Web/stats.json",
    "analyze:source": "npm run build:dev && source-map-explorer dist/WaiterRobot-Web/**/*.js",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "ngcc": "ngcc --properties es2020 browser module main --async false --first-only --create-ivy-entry-points",
    "prepare": "husky install"
  },
  "private": true,
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "dependencies": {
    "@angular/animations": "14.1.0",
    "@angular/cdk": "14.1.0",
    "@angular/common": "14.1.0",
    "@angular/compiler": "14.1.0",
    "@angular/core": "14.1.0",
    "@angular/flex-layout": "14.0.0-beta.40",
    "@angular/forms": "14.1.0",
    "@angular/localize": "14.1.0",
    "@angular/platform-browser": "14.1.0",
    "@angular/platform-browser-dynamic": "14.1.0",
    "@angular/router": "14.1.0",
    "@ng-bootstrap/ng-bootstrap": "13.0.0",
    "@ngx-loading-bar/core": "6.0.2",
    "@ngx-loading-bar/router": "6.0.2",
    "@popperjs/core": "2.11.5",
    "@zxing/browser": "0.1.1",
    "@zxing/library": "0.19.1",
    "@zxing/ngx-scanner": "3.6.2",
    "angularx-qrcode": "14.0.0",
    "bootstrap": "5.2.0",
    "dfx-bootstrap-table": "1.2.1",
    "dfx-helper": "4.5.3",
    "dfx-translate": "1.2.1",
    "ngx-bootstrap-icons": "1.9.1",
    "rxjs": "7.5.6",
    "tslib": "2.4.0",
    "zone.js": "0.11.7"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "14.1.0",
    "@angular-eslint/builder": "14.0.2",
    "@angular-eslint/eslint-plugin": "14.0.2",
    "@angular-eslint/eslint-plugin-template": "14.0.2",
    "@angular-eslint/schematics": "14.0.2",
    "@angular-eslint/template-parser": "14.0.2",
    "@angular/cli": "14.1.0",
    "@angular/compiler-cli": "14.1.0",
    "@types/jasmine": "4.0.3",
    "@types/node": "18.6.3",
    "@typescript-eslint/eslint-plugin": "5.32.0",
    "@typescript-eslint/parser": "5.32.0",
    "eslint": "8.21.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "latest",
    "eslint-plugin-jsdoc": "latest",
    "eslint-plugin-prefer-arrow": "latest",
    "husky": "8.0.1",
    "jasmine-core": "4.3.0",
    "jasmine-spec-reporter": "7.0.0",
    "karma": "6.4.0",
    "karma-chrome-launcher": "3.1.1",
    "karma-coverage": "2.2.0",
    "karma-jasmine": "5.1.0",
    "karma-jasmine-html-reporter": "2.0.0",
    "prettier": "2.7.1",
    "pretty-quick": "3.1.3",
    "protractor": "7.0.0",
    "source-map-explorer": "2.5.2",
    "ts-node": "10.9.1",
    "typescript": "4.7.4",
    "webpack-bundle-analyzer": "4.5.0"
  }
}
