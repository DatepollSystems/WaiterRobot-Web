{
  "name": "waiter-robot-web",
  "version": "0.0.1-preview",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build:prod": "rm -rf ./dist/WaiterRobot-Web/ && mkdir -p ./dist/WaiterRobot-Web && ng build --configuration production && cp ./LICENSE ./dist/WaiterRobot-Web/",
    "build:testing": "rm -rf ./dist/WaiterRobot-Web/ && mkdir -p ./dist/WaiterRobot-Web && ng build --configuration testing && cp ./LICENSE ./dist/WaiterRobot-Web/",
    "build:dev": "rm -rf ./dist/WaiterRobot-Web/ && mkdir -p ./dist/WaiterRobot-Web && ng build && cp ./LICENSE ./dist/WaiterRobot-Web/",
    "build:stats": "rm -rf ./dist/WaiterRobot-Web/ && mkdir -p ./dist/WaiterRobot-Web && ng build --stats-json",
    "analyze": "webpack-bundle-analyzer dist/WaiterRobot-Web/stats.json",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "ngcc": "ngcc --properties es2015 browser module main --async false --first-only --create-ivy-entry-points",
    "prepare": "husky install"
  },
  "private": true,
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "dependencies": {
    "@angular/animations": "13.3.8",
    "@angular/cdk": "13.3.7",
    "@angular/common": "13.3.8",
    "@angular/compiler": "13.3.8",
    "@angular/core": "13.3.8",
    "@angular/flex-layout": "13.0.0-beta.38",
    "@angular/forms": "13.3.8",
    "@angular/localize": "13.3.8",
    "@angular/platform-browser": "13.3.8",
    "@angular/platform-browser-dynamic": "13.3.8",
    "@angular/router": "13.3.8",
    "@ng-bootstrap/ng-bootstrap": "12.1.1",
    "@zxing/browser": "0.1.1",
    "@zxing/library": "0.19.1",
    "@zxing/ngx-scanner": "3.5.0",
    "angularx-qrcode": "13.0.15",
    "bootstrap": "5.1.3",
    "dfx-bootstrap-table": "1.1.2",
    "dfx-helper": "4.3.0",
    "dfx-translate": "1.0.4",
    "ngx-bootstrap-icons": "1.8.3",
    "ngx-print": "1.2.1",
    "rxjs": "6.6.7",
    "tslib": "2.4.0",
    "zone.js": "0.11.5"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "13.3.5",
    "@angular-eslint/builder": "13.2.1",
    "@angular-eslint/eslint-plugin": "13.2.1",
    "@angular-eslint/eslint-plugin-template": "13.2.1",
    "@angular-eslint/schematics": "13.2.1",
    "@angular-eslint/template-parser": "13.2.1",
    "@angular/cli": "13.3.5",
    "@angular/compiler-cli": "13.3.8",
    "@types/jasmine": "3.10.3",
    "@types/node": "16.11.24",
    "@typescript-eslint/eslint-plugin": "5.23.0",
    "@typescript-eslint/parser": "5.23.0",
    "eslint": "8.15.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "latest",
    "eslint-plugin-jsdoc": "latest",
    "eslint-plugin-prefer-arrow": "latest",
    "husky": "7.0.4",
    "jasmine-core": "3.99.0",
    "jasmine-spec-reporter": "7.0.0",
    "karma": "6.3.20",
    "karma-chrome-launcher": "3.1.1",
    "karma-coverage": "2.2.0",
    "karma-jasmine": "4.0.2",
    "karma-jasmine-html-reporter": "1.7.0",
    "prettier": "2.6.2",
    "pretty-quick": "3.1.3",
    "protractor": "7.0.0",
    "ts-node": "10.7.0",
    "typescript": "4.5.5"
  }
}
