{
  "name": "waiter-robot-web",
  "version": "0.2.0-beta",
  "scripts": {
    "ng": "ng",
    "start": "npm run translate:types && ng serve",
    "build:prod": "rm -rf ./dist/WaiterRobot-Web/ && mkdir -p ./dist/WaiterRobot-Web && ng build --configuration production && cp ./LICENSE ./dist/WaiterRobot-Web/",
    "build:testing": "rm -rf ./dist/WaiterRobot-Web/ && mkdir -p ./dist/WaiterRobot-Web && ng build --configuration testing && cp ./LICENSE ./dist/WaiterRobot-Web/",
    "build:dev": "rm -rf ./dist/WaiterRobot-Web/ && mkdir -p ./dist/WaiterRobot-Web && ng build && cp ./LICENSE ./dist/WaiterRobot-Web/",
    "build:stats": "rm -rf ./dist/WaiterRobot-Web/ && mkdir -p ./dist/WaiterRobot-Web && ng build --stats-json",
    "analyze": "npm run build:stats && webpack-bundle-analyzer dist/WaiterRobot-Web/stats.json",
    "analyze:source": "npm run build:dev && source-map-explorer dist/WaiterRobot-Web/**/*.js",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "ngcc": "ngcc --properties es2020 browser module main --async false --first-only --create-ivy-entry-points",
    "prepare": "husky install && npm run translate:types",
    "translate:types": "npm exec generateTranslationType ./src/assets/i18n/de.json",
    "translate:reset": "npm exec resetTranslationType"
  },
  "type": "module",
  "private": true,
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "dependencies": {
    "@angular/animations": "14.2.3",
    "@angular/cdk": "14.2.2",
    "@angular/common": "14.2.3",
    "@angular/compiler": "14.2.3",
    "@angular/core": "14.2.3",
    "@angular/flex-layout": "14.0.0-beta.40",
    "@angular/forms": "14.2.3",
    "@angular/localize": "14.2.3",
    "@angular/platform-browser": "14.2.3",
    "@angular/platform-browser-dynamic": "14.2.3",
    "@angular/router": "14.2.3",
    "@ng-bootstrap/ng-bootstrap": "13.0.0",
    "@ngx-loading-bar/core": "6.0.2",
    "@ngx-loading-bar/router": "6.0.2",
    "@popperjs/core": "2.11.6",
    "@swimlane/ngx-charts": "^20.1.0",
    "@zxing/browser": "0.1.1",
    "@zxing/library": "0.19.1",
    "@zxing/ngx-scanner": "3.6.2",
    "angularx-qrcode": "14.0.0",
    "bootstrap": "5.2.1",
    "dfx-bootstrap-table": "1.3.0",
    "dfx-helper": "4.6.0",
    "dfx-translate": "1.3.p1",
    "ngx-bootstrap-icons": "1.9.1",
    "remove": "^0.1.5",
    "rxjs": "7.5.6",
    "tslib": "2.4.0",
    "zone.js": "0.11.8"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "14.2.3",
    "@angular-eslint/builder": "14.1.2",
    "@angular-eslint/eslint-plugin": "14.1.2",
    "@angular-eslint/eslint-plugin-template": "14.1.2",
    "@angular-eslint/schematics": "14.1.2",
    "@angular-eslint/template-parser": "14.1.2",
    "@angular/cli": "14.2.3",
    "@angular/compiler-cli": "14.2.3",
    "@types/d3-scale": "^4.0.2",
    "@types/d3-selection": "^3.0.3",
    "@types/jasmine": "4.3.0",
    "@typescript-eslint/eslint-plugin": "5.38.0",
    "@typescript-eslint/parser": "5.38.0",
    "eslint": "8.23.1",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "latest",
    "eslint-plugin-jsdoc": "latest",
    "eslint-plugin-prefer-arrow": "latest",
    "husky": "8.0.1",
    "jasmine-core": "4.4.0",
    "jasmine-spec-reporter": "7.0.0",
    "karma": "6.4.1",
    "karma-chrome-launcher": "3.1.1",
    "karma-coverage": "2.2.0",
    "karma-jasmine": "5.1.0",
    "karma-jasmine-html-reporter": "2.0.0",
    "node-fetch": "2.6.7",
    "prettier": "2.7.1",
    "pretty-quick": "3.1.3",
    "protractor": "7.0.0",
    "source-map-explorer": "2.5.2",
    "ts-node": "10.9.1",
    "typescript": "4.7.4",
    "webpack-bundle-analyzer": "4.6.1"
  }
}
