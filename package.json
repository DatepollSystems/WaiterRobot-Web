{
  "name": "waiter-robot-web",
  "version": "0.0.1-preview",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build:prod": "rm -rf ./dist/WaiterRobot-Web/ && mkdir -p ./dist/WaiterRobot-Web && ng build --configuration production && cp ./LICENSE ./dist/WaiterRobot-Web/",
    "build:testing": "rm -rf ./dist/WaiterRobot-Web/ && mkdir -p ./dist/WaiterRobot-Web && ng build --configuration testing && cp ./LICENSE ./dist/WaiterRobot-Web/",
    "build:dev": "rm -rf ./dist/WaiterRobot-Web/ && mkdir -p ./dist/WaiterRobot-Web && ng build && cp ./LICENSE ./dist/WaiterRobot-Web/",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "ngcc": "ngcc --properties es2015 browser module main --async false --first-only --create-ivy-entry-points",
    "prepare": "husky install"
  },
  "private": true,
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "dependencies": {
    "@angular/animations": "^13.1.1",
    "@angular/cdk": "^13.1.1",
    "@angular/common": "^13.1.1",
    "@angular/compiler": "^13.0.3",
    "@angular/core": "^13.1.1",
    "@angular/flex-layout": "^13.0.0-beta.36",
    "@angular/forms": "^13.1.1",
    "@angular/localize": "^13.1.1",
    "@angular/platform-browser": "^13.1.1",
    "@angular/platform-browser-dynamic": "^13.1.2",
    "@angular/router": "^13.1.1",
    "@ng-bootstrap/ng-bootstrap": "12.0.0-beta.4",
    "bootstrap": "^5.1.3",
    "dfx-bootstrap-table": "^1.0.5",
    "dfx-helper": "^4.0.12",
    "dfx-translate": "^1.0.3",
    "ng-qrcode": "^6.0.0",
    "ngx-bootstrap-icons": "^1.7.2",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^13.1.3",
    "@angular-eslint/builder": "13.0.1",
    "@angular-eslint/eslint-plugin": "13.0.1",
    "@angular-eslint/eslint-plugin-template": "13.0.1",
    "@angular-eslint/schematics": "13.0.1",
    "@angular-eslint/template-parser": "13.0.1",
    "@angular/cli": "^13.1.3",
    "@angular/compiler-cli": "^13.1.1",
    "@types/jasmine": "^3.10.3",
    "@types/node": "^16.11.20",
    "@typescript-eslint/eslint-plugin": "4.33.0",
    "@typescript-eslint/parser": "4.33.0",
    "eslint": "^7.26.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "latest",
    "eslint-plugin-jsdoc": "latest",
    "eslint-plugin-prefer-arrow": "latest",
    "husky": "^7.0.2",
    "jasmine-core": "~3.99.0",
    "jasmine-spec-reporter": "~7.0.0",
    "karma": "~6.3.11",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.1.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.7.0",
    "prettier": "^2.5.1",
    "pretty-quick": "^3.1.3",
    "protractor": "~7.0.0",
    "ts-node": "~10.4.0",
    "typescript": "~4.4.4"
  }
}
